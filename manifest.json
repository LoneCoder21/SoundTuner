{
    "manifest_version": 3,
    "name": "SoundTuner",
    "version": "1.0",

    "description": "Extension to manipulate background sounds.",
    "homepage_url": "https://github.com/LoneCoder21/SoundTuner",
    "browser_specific_settings": {
        "gecko": {
            "id": "abhinavneelam949@gmail.com"
        }
    },
    "icons": {
        "48": "icon.png",
        "96": "icon.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_title": "SoundTuner",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "scripting",
        "webNavigation",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "host_permissions": ["<all_urls>"]
}
