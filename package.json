{
    "name": "soundtuner",
    "version": "1.3.2",
    "description": "Extension to manipulate background sounds.",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "web-ext run --devtools",
        "start-chromium": "web-ext run --devtools --target=chromium",
        "build": "web-ext lint && web-ext build --overwrite-dest",
        "zip": "npm-build-zip --source=. --destination=build/ --name_only=true --name=soundtuner",
        "build-zip": "npm run build && npm run zip"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LoneCoder21/SoundTuner.git"
    },
    "keywords": [
        "sound"
    ],
    "author": "Abhinav Neelam <abhinavneelam949@gmail.com> (https://github.com/LoneCoder21)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/LoneCoder21/SoundTuner/issues"
    },
    "homepage": "https://github.com/LoneCoder21/SoundTuner#readme",
    "devDependencies": {
        "npm-build-zip": "^1.0.4",
        "prettier": "3.0.3",
        "webextension-polyfill": "^0.10.0"
    }
}
